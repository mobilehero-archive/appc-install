{
  "name": "@titanium/appcelerator",
  "version": "6.0.19",
  "description": "â­  Axway Amplify command-line (CLI) tool for installing Appcelerator Titanium SDK",
  "main": "index.js",
  "author": "Jeff Haynie",
  "license": "SEE LICENSE IN LICENSE",
  "preferGlobal": true,
  "bin": {
    "appc": "./bin/appc",
    "appcelerator": "./bin/appc"
  },
  "scripts": {
    "test": "cross-env APPC_TEST=1 JUNIT_REPORT_PATH=junit_report.xml nyc mocha test/**/*_test.js",
    "lint": "eslint . --fix",
    "list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prep": "npm install --ignore-scripts",
    "test2": "npm run prep",
    "prepublishOnly": "git pull && npm run prep && bump insane",
    "prepublishOnly2": "git pull && npm run prep && npm audit fix && bump insane"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobilehero-archive/appc-install.git"
  },
  "homepage": "https://github.com/appcelerator/appc-install",
  "keywords": [
    "appcelerator",
    "titanium",
    "apibuilder",
    "acs",
    "appcelerator cloud",
    "arrow"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-plugin-mocha": "^10.0.3",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "mocha-jenkins-reporter": "^0.4.7",
    "nyc": "^15.1.0",
    "should": "^13.2.3"
  },
  "dependencies": {
    "@titanium/titanium": "^5.4.23",
    "async": "^3.2.3",
    "chalk": "^5.0.0",
    "debug": "^4.3.3",
    "npm-install-checks": "^4.0.0",
    "pac-proxy-agent": "^5.0.0",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "semver": "^7.3.5",
    "strip-ansi": "^7.0.1",
    "tar": "^6.1.11"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
